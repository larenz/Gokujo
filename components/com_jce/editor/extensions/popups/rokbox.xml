<?xml version="1.0" ?>
<install version="2.0" type="extension" extension="rokbox" folder="popups">
    <name>WF_POPUPS_ROKBOX_TITLE</name>
    <version>2.1.0</version>
    <creationDate>31 May 2013</creationDate>
    <author>Ryan Demmer</author>
    <authorEmail>@@email@@</authorEmail>
    <authorUrl>http://www.joomlacontenteditor.net/</authorUrl>
    <copyright>Copyright (C) 2006 - 2013 Ryan Demmer. All rights reserved</copyright>
    <license>GNU/GPL Version 2 - http://www.gnu.org/licenses/gpl-2.0.html</license>
    <description>WF_POPUPS_ROKBOX_DESC</description>
    <files>
        <file>rokbox.php</file>
        <folder>rokbox</folder>
    </files>
    <params group="rokbox">
        <param name="enable" type="list" default="1" label="WF_LABEL_EXTENSION_ENABLE" description="WF_LABEL_EXTENSION_ENABLE_DESC">
            <option value="1">WF_OPTION_YES</option>
            <option value="0">WF_OPTION_NO</option>
        </param>
    </params>
    <media></media>
    <plugins>link,imgmanager_ext,mediamanager</plugins>
    <languages>
        <language tag="en-GB">en-GB.com_jce_popups_rokbox.ini</language>
    </languages>
</install>
 